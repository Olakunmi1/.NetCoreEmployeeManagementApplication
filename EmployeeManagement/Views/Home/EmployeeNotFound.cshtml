@model int

@{
    ViewBag.Title = "404 Error";
}

<div class="alert alert-danger mt-1 mb-1">
    <h4>404 Not Found Error:</h4>
    <hr />
    <h5>
        Employee With Id = @Model cannot be Found
    </h5>
</div>
<a class="btn btn-outline-success" asp-action="Index" asp-controller="home" style="width:auto">
    Click here to see the list of all employees</a>